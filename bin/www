const app = require('../app')
var port = normalizePort(process.env.PORT || '3001');
app.set('port', port);

app.listen(port, () => {
  console.log('Express server listening on port ' + port);
});


function normalizePort(val) {
  var port = parseInt(val, 10);

  if (isNaN(port)) {
    return val;
  }

  if (port >= 0) {
    return port;
  }

  return false;
}




